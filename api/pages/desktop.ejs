<div id='desktop' class='desktop no-select' style="display: none">
	<div class='dt-area'>
		<div class='back-plane'>
			<div class='text'>
				<p id='client-ver'>Clouds</p>
				<p id='api-ver'></p>
			</div>
		</div>
		<div class='windows'></div>
	</div>
	<div class='taskbar'>
		<button class='button apps-btn'></button>
		<button class='button fullscreen-btn'></button>
		<div class='tasks'></div>
		<div id='taskbar-apps-menu'>
			<ul></ul>
			<hr/>
			<button class='button restart-btn'>Restart</button>
			<button class='button logout-btn'>Logout</button>
		</div>
		<ul id='taskbar-window-list'></ul>
	</div>
	<div class='context-menu'></div>
	<div id='window-drag-rect'></div>
</div>
<template id='t_window'>
	<div class='window no-select'>
		<div class='window-head'>
			<button class='button options-btn'></button>
			<span class='window-title'>Window</span>
			<button class='button minimize-btn'></button>
			<button class='button maxrestore-btn'></button>
			<button class='button close-btn'></button>
		</div>
		<div class='window-body'></div>
	</div>
</template>