{
	"plugins": [
		// Point /@sys/ references to dummy imports in public js includes
		["module-resolver", {
			"alias": {
				"/@sys": "./client/public/mcore/",
				"/@comm": "./client/public/mcomm/"
			}
		}]
	],
	"presets": [
		["@babel/preset-env", {		
			"targets": {
				"chrome": 63
			},
			"useBuiltIns": "usage",
			"corejs": "^3.36.1"
		}]
	]
}