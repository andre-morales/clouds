{
	"compilerOptions": {
		"strict": false,
		"module": "ESNext",
		"target": "ES2022",
		"moduleResolution": "Bundler",
		"emitDeclarationOnly": true,
		"outDir": "types",
		"composite": true,
		"incremental": true,
		"paths": {
			"/@sys/*": ["./src/*"]
		}
	},
	"include": ["src"],
	"exclude": [
		"node_modules",
		"**/node_modules/*"
	]
}