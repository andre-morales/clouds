{
	"fs": {
		"/sys/": {
			"path": "/",
			"access": "@all"
		},
		"/d/": {
			"path": "/",
			"access": "@all"
		},
		"/usr/": {
			"path": "./usr/$user/",
			"acess": "@all"
		},
		"/trash/": {
			"path": "./usr/$user/trash/",
			"acess": "@all"
		}
	},
	
	"extensions": {
		"shell": {
			"enabled": true,
			"exec": "cmd.exe"
		},
		"ffmpeg": {
			"enabled": true,
			"ffmpeg_exec": "ffmpeg",
			"ffprobe_exec": "ffprobe"
		},
		"mediastr": {
			"enabled": true,
			"ytdl": "python extensions/yt-dlp.py",
			"downpath": "downloads"
		}
	}
}