{
	"fs": {
		"/sys/": {
			"path": "/",
			"access": "@all"
		},
		"/d/": {
			"path": "/data/",
			"access": "@all"
		},
		"/usr/": {
			"path": "./usr/$user/",
			"acess": "@all"
		}
	},
	
	"extensions": {
		"shell": {
			"enabled": true,
			"exec": "bash"
		},
		"ffmpeg": {
			"enabled": true,
			"ffmpeg_exec": "ffmpeg",
			"ffprobe_exec": "ffprobe"
		}
	}
}