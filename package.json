{
  "name": "clouds",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "api/runtime/main.mjs",
  "type": "module",
  "scripts": {
    "build-api-dist": "tsc -p api",
    
    "build-client-dev": "webpack --config client/webpack.config.cjs --env development",
    "build-client-prod": "webpack --config client/webpack.config.cjs --env production",
    "build-client-types": "tsc -b client",
    "check-client-stats": "webpack --config client/webpack.config.cjs --env production --json > client/res/pack/.stats.json && npx webpack-bundle-analyzer client/res/pack/.stats.json",
    
    "build-apps-dev": "webpack --config apps/webpack.config.cjs --env development",
    "build-apps-prod": "webpack --config apps/webpack.config.cjs --env production",
    "check-apps-types": "tsc -b apps",
    "check-apps-stats": "webpack --config apps/webpack.config.cjs --env production --json > apps/.stats.json && npx webpack-bundle-analyzer apps/.stats.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/zepto": "^1.0.36",
    "chalk": "^5.3.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "ejs": "^3.1.9",
    "express": "^4.19.2",
    "express-fileupload": "^1.4.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.0",
    "@types/morgan": "^1.9.9",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "core-js": "^3.36.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4"
  }
}
